{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "routes": [
    { "src": "/model/(?<id>[^/]+)$", "dest": "/model?id=$id" },
    { "src": "/accessory/(?<id>[^/]+)$", "dest": "/accessory?id=$id" },
    { "src": "/info/(?<id>[^/]+)$", "dest": "/info?id=$id" },
    { "src": "/cart/(?<step>[^/]+)$", "dest": "/cart?step=$step" }
  ]
}
